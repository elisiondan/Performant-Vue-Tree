    "docs:dev": "vuepress dev docs",
    "build:example": "vue-cli-service build ./fi-api-data/main.ts --modern",
    "run:example": "npm run build:example && npx http-serve ./example",
    "postbuild-scripts": "npm run emit-declarations && node scripts/index.js",
    "emit-declarations": "tsc -p ./tsconfig.build.json --emitDeclarationOnly"
  },
